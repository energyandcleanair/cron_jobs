{
  "command": "refresh_weather_measurements",
  "scraper": "earthengine",
  "filters": [
    {
      "iso2": ["EU", "CN", "IN"],
      "region_type": "country",
      "date_from": -10,
      "variable": ["CDD", "HDD", "heat_index"]

    },
    {
      "iso2": ["CN", "IN"],
      "region_type": "gadm1",
      "date_from": -10,
      "variable": ["solar_radiation", "wind_speed", "wind_power_curve", "wind_power_density", "precipitation", "temperature"]
    },
    {
      "iso2": ["ID", "IN"],
      "region_type": "gadm1",
      "date_from": -10,
      "variable": ["thermal_anomaly"]
    }
  ]
}