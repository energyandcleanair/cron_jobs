{
  "command": "refresh_weather_measurements",
  "scraper": "earthengine",
  "filters": [
    {
      "iso2": ["EU", "CN", "IN"],
      "region_type": "country",
      "date_from": -10
    },
    {
      "iso2": ["CN", "IN"],
      "region_type": "gadm1",
      "date_from": -10
    },
    {
      "iso2": ["ID", "IN"],
      "region_type": "gadm1",
      "date_from": -10,
      "variable": ["thermal_anomaly"]
    }
  ]
}