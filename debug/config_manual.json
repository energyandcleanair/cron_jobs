{
  "command": "refresh_weather_measurements",
  "scraper": "earthengine",
  "filters": [
    {
      "iso2": [
        "EU"
      ],
      "region_type": "country",
      "date_from": "2018-01-01",
      "date_to": -5,
      "date_direction": -1,
      "variable": [
        "solar_radiation",
        "wind_speed",
        "wind_power_curve",
        "wind_power_density",
        "precipitation"
      ]
    }
  ]
}