{
  "command": "refresh_weather_measurements",
  "scraper": "earthengine",
  "filters": [
    {
      "iso2": ["CN"],
      "region_type": "gadm1",
      "date_from": "2019-01-01",
      "date_to": "2022-04-18",
      "date_direction": "backward",
      "variable": ["wind_power_density", "wind_power_curve"]
    }
  ]
}
