{
  "command": "refresh_weather_measurements",
  "scraper": "earthengine",
  "filters": [
    {
      "iso2": [
        "GB"
      ],
      "region_type": "country",
      "date_from": "2020-01-01",
      "date_direction": "backward",
      "variable": [
        "CDD",
        "HDD",
        "heat_index",
        "precipitation"
      ]
    }
  ]
}